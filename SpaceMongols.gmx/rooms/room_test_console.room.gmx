console_println('Hello world!');
console_println('How are you doing?');
console_println('I am well.');
unitTest_begin();
unitTest_assert('str_format("Hello, @!", "world") == "Hello, world!"',
        str_format('Hello, @!', 'world'), 'Hello, world!', 'string');
unitTest_assert('str_format("@@@",1,2,3) == "123"',
        str_format('@@@',1,2,3), '123', 'string');
unitTest_assert('str_format("@ Mississippi, @ Mississippi, @ Mississippi")',
        str_format('@ Mississippi, @ Mississippi, @ Mississippi', 1, 'two', 3),
        '1 Mississippi, two Mississippi, 3 Mississippi', 'string');
unitTest_assert('toString([1, 2, 3]) == "[1, 2, 3]"',
        toString(List(1, 2, 3), 'list:real'), 'list[1, 2, 3]', 'string');

unitTest_assert('testing startsWith',
        str_startsWith('hello','hell'), true, 'real');
unitTest_assert('startsWith 2',
        str_startsWith('onetwo', 'one'), true, 'real');
unitTest_assert('startsWith 3',
        str_startsWith('onetwo', 'net'), false, 'real');
    
unitTest_assert('lstrip 1',
        str_lstrip('  hi', '  '), 'hi', 'string');
unitTest_assert('lstrip 2',
        str_lstrip('map:real,real', 'map:'), 'real,real', 'string');
unitTest_assert('parseType() 1',
        parseType('map:real,real'), 'map:real,real', 'string');
unitTest_assert('parseType() 2',
        parseType('real,real'), 'real', 'string');
    
unitTest_assert('1+1=2',
        1+1, 2, 'real');

unitTest_assert('testing string addition',
        'hello ' + 'world', 'hello world', 'string');
    
unitTest_assert('arrays 1',
        Array(1, 2, 3), Array(1, 2, 3), 'array:real');
unitTest_assert('arrays 2',
        toString(Array(1, 2, 3), 'array:real'), '[1, 2, 3]', 'string');
    
map1 = Map(1, 'hello', 2, 'world');
unitTest_assert('Map 1', map1[?1], 'hello', 'string');
unitTest_assert('Map 2', map1[?2], 'world', 'string');
unitTest_assert('Map 3', ds_map_size(map1), 2, 'real');
unitTest_assert('Map comparison 1',
        Map(1, 'Hello', 2, 'World'), Map(1, 'Hello', 2, 'World'), 
        'map:real,string');
unitTest_assert('Map comparison 2',
        equals(map1, Map(1, 'Hello', 2, 'World'), 'map:string,real'), false, 
        'real');
unitTest_assert('Map comparison 3',
        equals(map1, Map(2, 'Hello', 3, 'World'), 'map:string,real'), false,
        'real');
unitTest_assert('Map comparison 4',
        equals(map1, Map(1, 'hello', 2, 'world'), 'map:real,string'), true, 
        'real');

unitTest_assert('toString({1: "hello", 2: "world"})',
        toString(Map(1, 'Hello', 2, 'World'), 'map:real,string'),
        'map{2: "World", 1: "Hello"}', 'string');

list1 = List(1, 2, 3, 4, 5);
unitTest_assert('[1, 2, 3, 4, 5].filter(x &gt; 3)', 
        ds_list_filter(list1, greaterThan, 3), List(4, 5), 'list:real');
unitTest_assert('filter 1',
    ds_list_filter(list1, equals, 4, 'real'), List(4), 'list:real');
    
unitTest_assert('split 1',
        str_split('1;2;3;4;5',';'), List('1','2','3','4','5'), 'list:string');
unitTest_assert('split 2',
        toString(str_split('1,2,3,4,5',','),'list:string'),
        'list["1", "2", "3", "4", "5"]', 'string');
unitTest_assert('split 3',
        str_split('1,2,3,4,5',';'), List('1,2,3,4,5'), 'list:string');

unitTest_assert('array slice 1',
    array_slice(Array(1, 2, 3), 2, 3), Array(3), 'array:real');
unitTest_assert('array slice 2',
    array_sliceFrom(Array(22, 56, 73, 24, 8), 3), Array(24, 8), 'array:real');
unitTest_assert('array slice 3',
    array_sliceTo(Array(22, 56, 73, 24, 8), 3), Array(22, 56, 73), 'array:real');

unitTest_end();</code>
